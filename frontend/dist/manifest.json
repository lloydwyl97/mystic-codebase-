  "related_applications": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "handle_links": "preferred",
  "protocol_handlers": [
    {
      "protocol": "web+mystic",
      "url": "/?action=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/upload",
      "accept": {
        "text/csv": [".csv"],
        "application/json": [".json"],
        "text/plain": [".txt"]
      }
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "permissions": [
    "notifications",
    "geolocation",
    "camera",
    "microphone",
    "background-sync",
    "persistent-storage"
  ],
  "features": [
    "Cross Platform",
    "fast",
    "simple",
    "secure",
    "offline-capable",
    "real-time-data",
    "ai-powered",
    "social-trading",
    "push-notifications",
    "background-sync",
    "file-handling",
    "share-target"
  ],
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "prefer_related_applications": false,
  "iarc_rating_id": "",
  "id": "mystic-trading-platform"
}
